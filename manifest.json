{
    "manifest_version": 3,
    "name": "Phishing Link Detector",
    "version": "1.0",
    "description": "Detects phishing websites and warns users.",
    "permissions": [
        "storage",
        "webNavigation",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon.png",
            "48": "icons/icon.png",
            "128": "icons/icon.png"
        }
    }
}
